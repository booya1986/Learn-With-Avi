{
  "name": "learnwithavi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "clean": "rm -rf .next .turbo node_modules/.cache",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "migrate:config": "tsx scripts/migrate-config-to-db.ts",
    "docs:generate": "typedoc",
    "docs:watch": "typedoc --watch",
    "docs:serve": "cd docs/api-reference && python3 -m http.server 8080",
    "docs:validate": "tsx scripts/validate-docs.ts",
    "docs:validate:strict": "tsx scripts/validate-docs.ts --strict",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "tsx scripts/validate-docs.ts --path"
    ]
  },
